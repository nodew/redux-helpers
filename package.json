{
  "name": "redux-helpers",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "test": "ava -v",
    "coverage": "nyc npm run test",
    "build": "rm lib/* && tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "nyc": "^11.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "ts-node": "^3.0.6",
    "typescript": "^2.3.4"
  },
  "ava": {
    "files": [
      "test/**/*.{ts,js}"
    ],
    "extensions": [
      ".ts",
      ".js"
    ],
    "tap": false,
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "**/*.spec.js",
      "**/node_modules/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./coverage",
    "all": true
  }
}
